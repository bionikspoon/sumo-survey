  "scripts": {
    "prestart": "yarn run migrate",
    "start": "slr .",
    "dev": "gulp dev --inspect",
    "test": "karma start --debug",
    "posttest": "nsp check",
    "test:watch": "karma start --debug --auto-watch --no-single-run",
    "coveralls": "[ -e ./coverage/lcov.txt ] && cat ./coverage/lcov.txt | coveralls || exit 1",
    "migrate": "./bin/migrate",
    "build": "gulp build",
    "build:debug": "yarn run build -- --debug",
    "prebuild:serve": "yarn run build",
    "build:serve": "yarn install strong-supervisor",
    "postbuild:serve": "yarn run start",
    "preheroku-postbuild": "yarn run build",
    "heroku-postbuild": "yarn install -g strong-supervisor",
    "postheroku-postbuild": "yarn prune --production"
  },

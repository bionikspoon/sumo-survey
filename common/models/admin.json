{
  "name": "Admin",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "": {
    "include": [
      "responses",
      {
        "choices": "responses"
      }
    ]
  },
  "scope": {
    "where": {
      "status": "active"
    }
  },
  "hidden": [
    "credentials",
    "challenges",
    "password",
    "email",
    "emailVerified"
  ],
  "mixins": {
    "Timestamp": {}
  },
  "properties": {
    "email": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "active"
    },
    "username": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "questions": {
      "type": "hasMany",
      "model": "Question",
      "foreignKey": "creatorId"
    }
  },
  "acls": [],
  "methods": {}
}

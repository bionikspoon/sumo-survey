{
  "name": "Choice",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Timestamp": {}
  },
  "properties": {
    "text": {
      "type": "string",
      "required": true
    },
    "order": {
      "type": "number",
      "required": true,
      "default": 50
    }
  },
  "validations": [],
  "relations": {
    "question": {
      "type": "belongsTo",
      "model": "Question",
      "foreignKey": "questionId"
    },
    "responses": {
      "type": "hasMany",
      "model": "Response",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
